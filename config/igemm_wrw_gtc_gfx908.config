gemm_n_per_block         = 128
gemm_k_per_block         = 16
wave_tile_m              = 64
wave_step_m              = 1
wave_repeat_m            = 2
wave_tile_n              = 32
wave_step_n              = 1
wave_repeat_n            = 2
tensor_a_thread_lengths  = [1,  4,  4,  1]      # N0xN1BxK0xK1
