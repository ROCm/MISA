[codegen]
arch = 'gfx908'
code_object = 'cov3'
mode = 'flat'

[igemm_wrw_gtc]
gemm_m_per_block         = 128
gemm_n_per_block         = 128
gemm_k_per_block         = 32
wave_tile_m              = 32
wave_step_m              = 1
wave_repeat_m            = 2
wave_tile_n              = 32
wave_step_n              = 1
wave_repeat_n            = 2
wave_tile_k              = 8
tensor_a_thread_lengths  = [1,  1,  1, 16]      # 1xNBx1xK
tensor_a_cluster_lengths = [1, 32,  1,  8]      # 1xNBx1xK
tensor_b_thread_lengths  = [1,  1,  1, 16]      # 1xNBx1xC
tensor_b_cluster_lengths = [1, 32,  1,  8]      # 1xNBx1xEC
direction                = "wrw"
precision                = "fp16"
tensor_layout            = 'nhwc'
nxb                      = 0
nxe                      = 1
gemm_k_global_split      = 0
