[codegen]
arch = 'gfx908'
code_object = 'cov3'
mode = 'flat'

#--------------------------- 128x64
[igemm_fwd_gtc]
gemm_m_per_block         = 128
gemm_n_per_block         = 64
gemm_k_per_block         = 16
wave_tile_m              = 32
wave_step_m              = 1
wave_repeat_m            = 1
wave_tile_n              = 32
wave_step_n              = 1
wave_repeat_n            = 2
wave_tile_k              = 2
tensor_a_pass_through    = 1
tensor_a_thread_lengths  = [1, 8, 1, 1]       # ExCxNB0xNB1
tensor_a_cluster_lengths = [1, 2, 4, 32]      # ExCxNB0xNB1
tensor_b_thread_lengths  = [1, 4, 1, 1]       # ExCxK0xK1
tensor_b_cluster_lengths = [1, 4, 1, 64]      # ExCxK0XK1
direction                = "fwd"
precision                = "fp32"
tensor_layout            = 'nhwc'
nxb                      = 0
nxe                      = 1
gemm_k_global_split      = 1