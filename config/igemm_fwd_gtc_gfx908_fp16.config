[codegen]
arch = 'gfx908'
code_object = 'cov3'
mode = 'flat'
data_type = 'fp16'

#--------------------------- 32x32
[igemm_fwd_gtc]
gemm_m_per_block         = 32
gemm_n_per_block         = 32
gemm_k_per_block         = 16
wave_tile_m              = 16
wave_step_m              = 1
wave_repeat_m            = 1
wave_tile_n              = 16
wave_step_n              = 1
wave_repeat_n            = 1
wave_tile_k              = 4
tensor_a_thread_lengths  = [1, 2, 1, 1]       # C0xC1ExK0xK1
tensor_a_cluster_lengths = [1, 8, 1, 32]       # C0xC1ExK0xK1
tensor_b_thread_lengths  = [1, 2, 1, 1]       # C0xC1ExN0xN1B
tensor_b_cluster_lengths = [1, 8, 1, 32]       # C0xC1ExN0xN1B
direction                = "fwd"
precision                = "fp16"
nxb                      = 1
nxe                      = 1

