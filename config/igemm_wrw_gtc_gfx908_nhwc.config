[codegen]
arch = 'gfx908'
code_object = 'cov3'
mode = 'flat'

#--------------------------- 256x32
[igemm_wrw_gtc]
gemm_m_per_block         = 256
gemm_n_per_block         = 32
gemm_k_per_block         = 16
wave_tile_m              = 32
wave_step_m              = 1
wave_repeat_m            = 2
wave_tile_n              = 32
wave_step_n              = 1
wave_repeat_n            = 1
wave_tile_k              = 2
tensor_a_thread_lengths  = [1,  1,  1, 16]      # 1xNBx1xK
tensor_a_cluster_lengths = [1, 16,  1, 16]      # 1xNBx1xK
tensor_b_thread_lengths  = [1,  1,  1,  2]      # 1xNBxExC
tensor_b_cluster_lengths = [1, 16,  1, 16]      # 1xNBxEXC
direction                = "wrw"
precision                = "fp32"
tensor_layout            = 'nhwc'
nxb                      = 0
nxe                      = 0
gemm_k_global_split      = 0
