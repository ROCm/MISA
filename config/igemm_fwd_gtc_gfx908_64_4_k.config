tensor_a_thread_lengths  = [1,  2, 16,  1]      # C0xC1ExK0xK1
tensor_a_cluster_lengths = [1,  16, 1,  4]      # C0xC1ExK0xK1
tensor_b_thread_lengths  = [1,  1,  2,  1]      # C0xC1ExN0xN1B
tensor_b_cluster_lengths = [1,  32, 1,  2]      # C0xC1ExN0xN1B
direction                = 'fwd'
precision                = 'fp32'
